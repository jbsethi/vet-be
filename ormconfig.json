{
  "synchronize": true,
  "logging": false,
  "entities": [
     "src/components/**/entity.ts"
  ],
  "cli": {
     "entitiesDir": "src/entity",
     "migrationsDir": "src/migration",
     "subscribersDir": "src/subscriber"
   },
  "migrations": [
     "src/migration/**/*.ts"
  ],
  "subscribers": [
     "src/subscriber/**/*.ts"
   ],
   "seeds": [
      "src/components/**/seeds{.ts,.js}"
   ],
   "factories": [
      "src/components/**/factories{.ts,.js}"
   ]
}